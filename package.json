{
  "name": "@9renpoto/slides",
  "description": "my slides",
  "version": "0.0.0",
  "dependencies": {
    "impress.js": "^0.6.0"
  },
  "devDependencies": {
    "@9renpoto/eslint-config": "^1.4.0",
    "@9renpoto/tslint-config": "^2.0.0",
    "awesome-typescript-loader": "^3.0.3",
    "css-loader": "^0.26.0",
    "eslint": "^3.9.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "ghpages": "0.0.8",
    "npm-run-all": "^4.0.0",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "tslint": "^4.0.0",
    "typescript": "^2.0.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^2.2.1"
  },
  "license": "MIT",
  "main": "dist/index.html",
  "private": true,
  "scripts": {
    "build": "npm-run-all build:*",
    "build:html": "pug src --out dist",
    "build:webpack": "webpack",
    "deploy": "ghpages --path dist",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint .",
    "postbuild": "cp -r src/img dist/img",
    "prebuild": "rimraf dist",
    "predeploy": "npm run build",
    "start": "webpack-dev-server --content-base dist/",
    "test": "npm-run-all lint build"
  }
}
