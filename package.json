{
  "name": "@9renpoto/slides",
  "description": "my slides",
  "version": "0.0.0",
  "dependencies": {
    "impress.js": "^0.6.0"
  },
  "devDependencies": {
    "@9renpoto/eslint-config": "^1.6.0",
    "@9renpoto/tslint-config": "^3.0.0",
    "awesome-typescript-loader": "^3.0.5",
    "cpx": "^1.5.0",
    "css-loader": "^0.27.0",
    "eslint": "^3.17.0",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "ghpages": "0.0.10",
    "npm-run-all": "^4.0.2",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.6.0",
    "style-loader": "^0.13.2",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1",
    "uglify-js": "^2.8.5",
    "uglifyjs-webpack-plugin": "^0.3.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "license": "MIT",
  "main": "dist/index.html",
  "private": true,
  "scripts": {
    "build": "npm-run-all clean -p build:* copy",
    "build:html": "pug src --out dist",
    "build:webpack": "webpack",
    "clean": "rimraf dist",
    "copy": "cpx \"src/**/*.png\" dist",
    "deploy": "ghpages --path dist",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint . --cache",
    "predeploy": "npm run build",
    "start": "webpack-dev-server --content-base dist/",
    "test": "npm-run-all lint build"
  }
}
